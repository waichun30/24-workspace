<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.center.tool.entity.QrCodeMapper">

    <resultMap id="QrCodeDO" type="com.center.tool.entity.model.QrCodeDO">
        <result column="qr_code" property="qrCode"/>
        <result column="merchant_code" property="merchantCode"/>
        <result column="qr_name" property="qrName"/>
        <result column="created_time" property="createdTime"/>
        <result column="modified_time" property="modifiedTime"/>
        <result column="extend_info" property="extendInfo"/>
    </resultMap>


    <select id="getQrCodeByQrCode" parameterType="String" resultMap="QrCodeDO">
        SELECT
            *
        FROM
            qr_code
        WHERE
            qr_code = #{qrCode}
    </select>

</mapper>